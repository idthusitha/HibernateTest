<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
	<persistence-unit name="sample">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		
		<class>com.pojo.ReportLabelMapping</class>
		
		<properties>
			<property name="javax.persistence.jdbc.driver" value="org.hibernate.dialect.PostgreSQLDialect" />
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://192.168.1.237:5432/PGDEV" />
			<property name="javax.persistence.jdbc.user" value="postgres" />
			<property name="javax.persistence.jdbc.password" value="" />

		</properties>

	</persistence-unit>
</persistence>